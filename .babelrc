{
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }],
    "stage-0"],
  "plugins": [
    "transform-es2015-destructuring",
    "transform-es2015-parameters",
    "transform-object-rest-spread",
    "add-module-exports",
    "transform-decorators-legacy"
  ],
  "ignore": [
    "node_modules/**/*.js"
  ]
}